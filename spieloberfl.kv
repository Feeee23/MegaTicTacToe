    canvas.before:
        Color:
            rgba: .5, .5, .5, 1

        Rectangle:
            pos: 2*self.width/49+0*16*self.width/49,self.height*0.8-9*self.width/49-0*16*self.width/49 
            size: 13*self.width/49, 13*self.width/49

        Rectangle:
            pos: 2*self.width/49+1*16*self.width/49,self.height*0.8-9*self.width/49-0*16*self.width/49 
            size: 13*self.width/49, 13*self.width/49

